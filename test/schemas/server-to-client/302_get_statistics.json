{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "success",
    "greydRule",
    "greydId",
    "totalScore",
    "totalWalkDistance",
    "totalGameTime",
    "collectedBait",
    "totalGame",
    "wonGame"
  ],
  "properties": {
    "success": {
      "$id": "#/properties/success",
      "type": "boolean",
      "title": "The Success Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "greydRule": {
      "$id": "#/properties/greydRule",
      "type": "integer",
      "title": "The Greydrule Schema",
      "default": 0,
      "examples": [
        302
      ]
    },
    "greydId": {
      "$id": "#/properties/greydId",
      "type": "integer",
      "title": "The Greydid Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "totalScore": {
      "$id": "#/properties/totalScore",
      "type": "integer",
      "title": "The Totalscore Schema",
      "default": 0,
      "examples": [
        2250
      ]
    },
    "totalWalkDistance": {
      "$id": "#/properties/totalWalkDistance",
      "type": "number",
      "title": "The Totalwalk Schema",
      "default": 0,
      "examples": [
        2354
      ]
    },
    "totalGameTime": {
      "$id": "#/properties/totalGameTime",
      "type": "number",
      "title": "The Totalgametime Schema",
      "default": "0",
      "examples": [
        "123456"
      ],
      "pattern": "^(.*)$"
    },
    "collectedBait": {
      "$id": "#/properties/collectedBait",
      "type": "integer",
      "title": "The Collectedbait Schema",
      "default": 0,
      "examples": [
        54
      ]
    },
    "totalGame": {
      "$id": "#/properties/totalGame",
      "type": "integer",
      "title": "The Totalgame Schema",
      "default": 0,
      "examples": [
        12
      ]
    },
    "wonGame": {
      "$id": "#/properties/wonGame",
      "type": "integer",
      "title": "The Wongame Schema",
      "default": 0,
      "examples": [
        5
      ]
    }
  }
}